if ('<%= @result.nil? %>' == 'true')
  $('#survey_result_attributes_id').val('')
  $('#survey_result_attributes_conclusion').val('')
  $('#survey_result_attributes_title').val('')
  $('#survey_result_attributes_rdate').val('')
  $('#survey_result_attributes_appointment').val('')
  $('#survey_result_attributes_causes').val('')
  $('#survey_result_attributes_first_inspection').val('')
  $('#survey_result_attributes_results').val('')

else
  $('#survey_result_attributes_id').val('<%= @result.try(:id) %>')
  $('#survey_result_attributes_conclusion').val('<%= @result.try(:conclusion) %>')
  $('#survey_result_attributes_title').val('<%= @result.try(:title) %>')
  $('#survey_result_attributes_rdate').val('<%= @result.try(:rdate) %>')
  $('#survey_result_attributes_appointment').val('<%= @result.try(:appointment) %>')
  $('#survey_result_attributes_causes').val('<%= @result.try(:causes) %>')
  $('#survey_result_attributes_first_inspection').val('<%= @result.try(:first_inspection) %>')
  $('#survey_result_attributes_results').val('<%= @result.try(:results) %>')
